<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= stylesheet_link_tag    'static_pages', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    'dashboard', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    'bank_system', media: 'all', 'data-turbolinks-track' => true %>
    <%= stylesheet_link_tag    'user_bank_interface', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <div class="row">
    <!-- Dashboard -->
        <div class="main">
          <%= yield %>
        </div>
    <!-- End Dashboard -->
      </div>
      <%= render 'layouts/footer' %>
      <%= debug(params) if Rails.env.development? %>
    </div>
  </body>
</html>
